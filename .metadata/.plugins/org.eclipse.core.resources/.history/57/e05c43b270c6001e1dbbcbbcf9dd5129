package doublecolonoperator;


@FunctionalInterface
interface inter{
	void m1(int a);
}
public class App01{
			
	public static void m1(int a) {
		System.out.println(a);
	}
	
	public static void main(String[] args) {
			// using lamda function
			inter i = (a)->System.out.println(a);
			i.m1(10);
			
			// using double colon operator
			inter i1 = App01::m1; 
			
			
	}
}

